{
  "name": "blitz-dock",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build:js": "esbuild assets/js/public.js --minify --outfile=assets/js/public.min.js --target=es2019 --legal-comments=none",
    "build:css": "csso assets/css/public.css --output assets/css/public.min.css",
    "build": "npm run build:js && npm run build:css"
  },
  "devDependencies": {
    "csso": "^5.0.5",
    "esbuild": "^0.20.2"
  }
}